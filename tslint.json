{
    "extends": "tslint:recommended",
    "rules": {
        "no-console": false,
        "no-namespace": false,
        "space-before-function-paren": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-pascal-case",
            "allow-leading-underscore"
        ]
    }
}